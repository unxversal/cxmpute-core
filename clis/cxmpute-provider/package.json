{
	"name": "@unxversallabs/cxmpute-provider",
	"version": "0.5.3",
	"description": "Transform your computer into an AI compute provider and earn rewards",
	"keywords": ["ai", "compute", "provider", "blockchain", "earnings", "cli"],
	"homepage": "https://cxmpute.cloud",
	"repository": {
		"type": "git",
		"url": "https://github.com/unxversal/cxmpute-core.git",
		"directory": "clis/cxmpute-provider"
	},
	"bugs": "https://github.com/unxversal/cxmpute-core/issues",
	"author": "Cxmpute Team <support@cxmpute.cloud>",
	"license": "MIT",
	"bin": {
		"cxmpute-provider": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"build:secure": "node scripts/build-with-secrets.js",
		"prepublishOnly": "npm run build",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist",
		"readme.md"
	],
	"dependencies": {
		"@peaq-network/sdk": "^0.9.2",
		"asciify": "^1.3.5",
		"cors": "^2.8.5",
		"country-state-city": "^3.2.1",
		"dotenv": "^16.4.5",
		"express": "^5.1.0",
		"figlet": "^1.8.1",
		"helmet": "^8.1.0",
		"ink": "^4.4.1",
		"ink-form": "^2.0.1",
		"ink-link": "^4.1.0",
		"ink-spinner": "^5.0.0",
		"kokoro-js": "^1.2.0",
		"meow": "^11.0.0",
		"morgan": "^1.10.0",
		"ngrok": "^5.0.0-beta.2",
		"node-localstorage": "^3.0.5",
		"ollama": "^0.5.15",
		"portfinder": "^1.0.37",
		"react": "^18.2.0",
		"turndown": "^7.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.23",
		"@types/figlet": "^1.7.0",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.0.0",
		"@types/node-localstorage": "^1.3.3",
		"@types/react": "^18.0.32",
		"@types/turndown": "^5.0.5",
		"@types/uuid": "^10.0.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"tsx": "^4.7.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"pkg": {
		"scripts": [
			"dist/**/*.js",
			"node_modules/**/*.js"
		],
		"assets": [
			"dist/**/*",
			"node_modules/**/*"
		],
		"targets": [
			"node18-linux-x64",
			"node18-macos-x64",
			"node18-macos-arm64",
			"node18-win-x64"
		],
		"outputPath": "binaries"
	}
}
